<svg xmlns="http://www.w3.org/2000/svg" width="{{ total_width }}" height="20">
  <rect width="{{ label_width }}" height="20" fill="#555"/>
  <rect x="{{ label_width }}" width="{{ status_width }}" height="20" fill="{{ color }}"/>
  <text x="{{ label_width // 2 }}" y="14" fill="#fff" font-family="Verdana" font-size="11" text-anchor="middle">{{ label }}</text>
  <text x="{{ label_width + (status_width // 2) }}" y="14" fill="#fff" font-family="Verdana" font-size="11" text-anchor="middle">{{ status }}</text>
</svg>
